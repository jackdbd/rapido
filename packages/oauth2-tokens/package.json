{
  "name": "@jackdbd/oauth2-tokens",
  "version": "0.0.0-canary-20250124161903",
  "description": "Useful functions when working with OAuth 2.0 tokens.",
  "author": {
    "name": "Giacomo Debidda",
    "email": "giacomo@giacomodebidda.com",
    "url": "https://giacomodebidda.com/"
  },
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "OAuth 2.0",
    "tokens"
  ],
  "type": "module",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*.d.ts"
      ]
    }
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "build": "run-s 'build:ts' 'build:readme' --print-label",
    "build:readme": "node ../scripts/dist/readme.js --package oauth2-tokens",
    "build:ts": "tsc -p tsconfig.json",
    "build:ts:watch": "tsc -p tsconfig.json --watch",
    "clean": "rimraf lib/ tsconfig.tsbuildinfo",
    "dev": "run-p build:ts:watch test:watch",
    "publint": "publint run .",
    "size": "pkg-size ./lib --sort-by=brotli --ignore-files {*.d.ts,*.map}",
    "test": "node --test --experimental-test-coverage",
    "test:ci": "node --test --test-reporter=@reporters/github --test-reporter-destination=stdout --test-reporter=spec --test-reporter-destination=stdout",
    "test:watch": "node --test --watch"
  },
  "dependencies": {
    "@jackdbd/indieauth": "0.0.0-canary-20250124153852",
    "@jackdbd/oauth2": "0.0.0-canary-20250124153852",
    "@jackdbd/schema-validators": "0.0.0-canary-20250124161903",
    "@sinclair/typebox": "^0.34.14",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "dayjs": "^1.11.13",
    "dayjs-plugin-utc": "^0.1.2",
    "jose": "^5.9.6",
    "ms": "3.0.0-canary.1",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {},
  "peerDependencies": {}
}
