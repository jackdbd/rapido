{
  "name": "@repo/stdlib",
  "version": "0.1.1",
  "private": true,
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./*": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:ts:watch": "tsc -p tsconfig.json --watch",
    "clean": "rimraf lib/ tsconfig.tsbuildinfo",
    "dev": "tsc -p tsconfig.json --watch",
    "pub": "echo '@repo/stdlib is a private package. Skip publishing it.'"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.14",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "json-schema-resolver": "^3.0.0",
    "jsonschema2mk": "^2.0.0"
  }
}
