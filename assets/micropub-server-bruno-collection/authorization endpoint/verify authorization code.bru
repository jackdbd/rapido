meta {
  name: verify authorization code
  type: http
  seq: 2
}

post {
  url: {{authorization_endpoint}}
  body: formUrlEncoded
  auth: none
}

body:form-urlencoded {
  client_id: {{client_id}}
  redirect_uri: {{redirect_uri}}
  code: {{code}}
  grant_type: authorization_code
  code_verifier: {{code_verifier}}
}
