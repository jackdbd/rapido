{
  "name": "@jackdbd/pkce",
  "version": "0.2.0-canary.5",
  "description": "Useful functions for implementing [Proof Key for Code Exchange (PKCE)](https://datatracker.ietf.org/doc/html/rfc7636).",
  "author": {
    "name": "Giacomo Debidda",
    "email": "giacomo@giacomodebidda.com",
    "url": "https://giacomodebidda.com/"
  },
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "OAuth 2.0",
    "PKCE"
  ],
  "type": "module",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "lib"
  ],
  "scripts": {
    "build": "run-s 'build:ts' 'build:docs' 'build:readme' --print-label",
    "build:docs": "node ../scripts/dist/docs.js --package pkce",
    "build:readme": "node ../scripts/dist/readme.js --package pkce",
    "build:ts": "tsc -p tsconfig.json",
    "build:ts:watch": "tsc -p tsconfig.json --watch",
    "clean": "rimraf lib/ tsconfig.tsbuildinfo",
    "dev": "run-p build:ts:watch test:watch",
    "lint": "eslint --config ../../eslint.config.mjs --fix",
    "publint": "publint run .",
    "size": "pkg-size ./lib --sort-by=brotli --ignore-files {*.d.ts,*.map}",
    "test": "node --test --experimental-test-coverage",
    "test:ci": "node --test --test-reporter=@reporters/github --test-reporter-destination=stdout --test-reporter=spec --test-reporter-destination=stdout",
    "test:watch": "node --test --watch"
  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.14",
    "seedrandom": "^3.0.5"
  },
  "devDependencies": {}
}
